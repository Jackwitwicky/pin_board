<h1>Welcome to the homepage.</h1>

<%= link_to "New Pin", new_pin_path %>

<% @pin.each do |pin| %>
    <p>
      <%= link_to pin.title, pin %>
      <%= link_to "Edit", edit_pin_path(pin) %>
      <%= link_to "Delete", pin_path(pin), method: :delete , data: {confirm: "Are you sure?"} %>
    </p>
<% end %>