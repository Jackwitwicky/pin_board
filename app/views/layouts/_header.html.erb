    <div class="container">
      <%= link_to "Pin Board", root_path, id: "logo" %>

      <nav>
        <ul class="nav navbar-nav navbar-right">
          <% if logged_in? %>
              <li><%= link_to "New Pin", new_pin_path %></li>
